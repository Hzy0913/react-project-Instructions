{
  "extends": "airbnb", // 使用eslint-config-airbnb eslint检测规则
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "restParams": true,
    "experimentalObjectRestSpread": true
  },
  "globals": {
    "store": false, //排除定义在window对象上的store
    "axios": false, //排除定义在window对象上的axios
    "$": false,  //排除定义在window对象上的$
    "__DEVTOOLS__": false,
    "__DMTEST__": false,
    "__DEVELOPMENT__": false
  },
  "rules": {
    "import/no-named-as-default": ["off"],
    "react/jsx-closing-tag-location": ["off"],
    "comma-dangle": ["off", "never"],
    "spaced-comment": ["off", "always"],
    "func-names": ["off"],
    "react/jsx-quotes": ["off"],
    "jsx-quotes": ["error"],
    "indent": ["error", 2, {"SwitchCase": 1}],
    "no-unused-expressions": ["off"],
    "max-len": ["warn", 100, 2, {"ignoreComments": true}],
    "object-curly-spacing": ["warn", "never"],
    /* 暂时关闭 */
    "react/no-danger": 0,
    "arrow-parens": 0, // 关闭箭头函数只有一个参数时需要有括号
    "array-callback-return": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "class-methods-use-this": 0,
    "consistent-return": 0,
    "global-require": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "jsx-a11y/img-has-alt": 0,
    "jsx-a11y/label-has-for": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "no-bitwise": 0,
    "no-case-declarations": 0,
    "no-confusing-arrow": 0,
    "no-mixed-operators": 0,
    "no-param-reassign": 0,
    "no-plusplus": 0,
    "no-return-assign": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "no-useless-concat": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "react/jsx-no-bind": 0,
    "react/jsx-no-target-blank": 0,
    "react/no-array-index-key": 0,
    "react/no-find-dom-node": 0,
    "react/no-string-refs": 0,
    "react/no-unused-prop-types": 0,
    "react/prefer-stateless-function": 0,
    "react/prop-types": 0,
    "react/require-default-props": 0,
    "react/sort-comp": 0,
  }
}
